#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
int main() {
	setlocale(LC_CTYPE, "RUS");
	puts("    *   **                   ***  *****                ****   ****   ");
	puts("  *   *    *                *    *  *    *           *      *      * ");
	puts("    *   **                  ***    ****                **       **   ");
	puts("  *      *                *    *****    *            *    *   *    * ");
	puts("  *      *                *   *      *  *            *  -   *   -  * ");
	puts("*          *                ****    ***              * -   -   -   * ");
	puts("*          *                *    **   *              *    -  -  -  * ");
	puts("*          *                *    *    *              * -   -    -  * ");
	puts("  *      *                    **   **                  *    -  - *   ");
	puts("    ****                         **                      *******     ");
	puts("                                                                     ");
	puts("                                                                     ");
	puts("                  ***                           *                    ");
	puts("                *     **                      *   *                  ");
	puts("                *  *     *                  *   *                    ");
	puts("              *    ****                     *   *                    ");
	puts("              *   **  *                   *     *                    ");
	puts("              *   **  ***                 *       ***                ");
	puts("                *   ***   *               **           *******       ");
	puts("                  *   *   *                  **              *       ");
	puts("                  *   ***                       **         **        ");
	puts("                    **                              ******           ");
	return 100;
}
